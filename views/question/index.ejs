<%- contentFor('title') %>Questions
<%- contentFor('body') %>

<h1>Questions</h1>

<a href="questions/new">New Questions</a>

<div class="blog_section">
    <% for (let record of records) { %>

      <div class="posts" id="<%= record.id %>">
      <h4><%= record.prompt %></h4>
      <a href="/questions/<%= record.id %>/edit">Edit</a>
      <br>
      <a href="/questions/<%= record.id %>/delete">Delete</a>
      </div>
      <br>
    <% } %>
  
</div>


