<%- contentFor('title') %>
<%- contentFor('body') %>
<h1> Delete resources page</h1>

<form method="post" action="/resources/<%= record.id %>">

    <label for="name">Name:</label><br/>
    <input type="text" id="name" name="name" value="<%= record.name %>" required><br/>

    <label for="orgtype">Type of organization:</label><br/>
    <input type="text" id="orgtype" name="orgtype"value="<%= record.orgtype %>" required><br/>

    <label for="contactperson">Contact Person:</label><br/>
    <input type="text" id="contactperson" name="contactperson"value="<%= record.contactperson %>" required></input><br/>

    <label for="phone">Phone Number:</label><br/>
    <input type="text" id="phone" name="phone"value="<%= record.phone %>" required></input><br/>

    <label for="address">Address:</label><br/>
    <input type="text" id="address" name="address"value="<%= record.address %>" required></input><br/>

    <label for="email">Email:</label><br/>
    <input type="text" id="email" name="email"value="<%= record.email %>" required></input><br/>

    <label for="website">Buisness Website:</label><br/>
    <input type="text" id="website" name="website"value="<%= record.website %>" required></input><br/>

    <br><button type="submit">Submit</button></<br>

  </form>
